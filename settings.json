        "[Default Dark Modern]": {
            "rules": {
                "builtin": "#42A5F5",
                "class": "#FFE082",
                "enum": "#FFE082",
                "function": "#42A5F5",
                "selfKeyword": "#E53935",
                "decorator": "#42A5F5",
                "operator": {
                    "foreground": "#80DEEA",
                    "fontStyle": ""
                },
                "keyword": "#CE93D8",
                "comments": "#757575",
                "number": "#FFA726",
                "string": "#AED581",
                "type": "#FFE082",
                "variable": "#E0E0E0",
                "macro":{
                    "foreground": "#F48FB1",
                    "fontStyle": "italic"
                }
            }
        }
    },
